external ( < ) : int -> int -> bool = "<";;
external ( - ) : int -> int -> int = "-";;
external ( + ) : int -> int -> int = "+";;

effect Get : unit -> int ;;
effect Put : int -> unit ;;

(let rec loop n =
  if 0 < n then perform (Put ((perform Get) + 1)); loop (n - 1) else ()
in match (loop 10) with
   | x -> (fun s -> x)
   | effect (Get x) k -> (fun s -> k s s)
   | effect (Put s) k -> (fun t -> k () s)
) 0
